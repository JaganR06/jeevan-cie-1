    "print(a)\n",
    "print(a1)\n",
    "\n",
    "#concatinating two arrays\n",
    "a_new=np.concatenate((a,a1),axis=1)\n",
    "print(\"\\nconcateinating two arr:\\n\",a_new)\n",
    "\n",
    "#revers of an array\n",
    "res=np.flip(a)\n",
